{
    "Fade In": {
        "prefix": "fade-in",
        "body": [
            "@keyframes fade-in{",
                "from{",
                    "opacity: 0;",
                "}",
                "to{",
                    "opacity: 1;",   
                "}",
            "}"
        ],
        "description": "Fade in keyframe"
    },
    "Fade in up": {
        "prefix": "fade-in-up",
        "body": [
            "@keyframes fade-in-up{",
                "from{",
                    "opacity: 0;",
                    "transform: translate3d(0, 100%, 0);",
                "}",
                "to{",
                    "opacity: 1;",
                    "transform: translate3d(0, 0, 0);",
                "}",
            "}"
        ],
        "description": "Fade in up keyframe"
    },
